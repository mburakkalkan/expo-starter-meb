{
	"name": "expo-starter",
	"version": "1.0.0",
	"private": true,
	"main": "expo-router/entry",
	"scripts": {
		"android": "expo start --android",
		"dev-build-all": "eas build --profile development",
		"dev-build-android": "eas build --platform android --profile development",
		"dev-build-ios": "eas build --platform ios --profile development",
		"dev-build-web": "eas build --platform web --profile development",
		"expo-install": "expo install",
		"ios": "expo start --ios",
		"lint": "expo lint",
		"local": "expo start --localhost",
		"offline": "expo start --offline",
		"prod-build-all": "eas build --profile production",
		"prod-build-android": "eas build --platform android --profile production",
		"prod-build-ios": "eas build --platform ios --profile production",
		"prod-build-web": "eas build --platform web --profile production",
		"reset-project": "node ./scripts/reset-project.js",
		"start": "expo start",
		"test": "jest --watchAll",
		"tunnel": "expo start --tunnel",
		"web": "expo start --web"
	},
	"jest": {
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.{ts,tsx,js,jsx}",
			"!**/coverage/**",
			"!**/node_modules/**",
			"!**/babel.config.js",
			"!**/expo-env.d.ts",
			"!**/.expo/**"
		],
		"preset": "jest-expo"
	},
	"dependencies": {
		"@expo/vector-icons": "^15.0.2",
		"@react-native-picker/picker": "2.11.1",
		"@react-navigation/bottom-tabs": "^7.4.0",
		"@react-navigation/drawer": "^7.1.1",
		"@react-navigation/elements": "^2.6.3",
		"@react-navigation/native": "^7.1.8",
		"axios": "^1.9.0",
		"expo": "^54.0.8",
		"expo-blur": "~15.0.7",
		"expo-camera": "~17.0.8",
		"expo-constants": "~18.0.9",
		"expo-dev-client": "~6.0.12",
		"expo-document-picker": "~14.0.7",
		"expo-font": "~14.0.8",
		"expo-haptics": "~15.0.7",
		"expo-image": "~3.0.8",
		"expo-image-picker": "~17.0.8",
		"expo-linking": "~8.0.8",
		"expo-router": "~6.0.6",
		"expo-splash-screen": "~31.0.10",
		"expo-status-bar": "~3.0.8",
		"expo-symbols": "~1.0.7",
		"expo-system-ui": "~6.0.7",
		"expo-web-browser": "~15.0.7",
		"nativewind": "^4.1.23",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.4",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-paper": "^5.13.1",
		"react-native-reanimated": "~4.1.0",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-svg": "15.12.1",
		"react-native-vector-icons": "^10.2.0",
		"react-native-web": "~0.21.0",
		"react-native-webview": "13.15.0",
		"react-native-worklets": "0.5.1",
		"tailwindcss": "^3.4.17"
	},
	"devDependencies": {
		"@babel/core": "^7.26.7",
		"@testing-library/react-native": "^13.2.0",
		"@types/jest": "^29.5.14",
		"@types/react": "~19.1.10",
		"eslint": "^9.25.0",
		"eslint-config-expo": "~10.0.0",
		"jest": "^29.7.0",
		"jest-expo": "~54.0.11",
		"ts-node": "^10.9.2",
		"typescript": "~5.9.2"
	},
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"listUnknownPackages": false
			}
		}
	}
}
